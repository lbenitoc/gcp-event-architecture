 # Ejemplo de configuración - Copiar a terraform.tfvars y personalizar

project_id = "TU_PROJECT_ID_AQUI"              # REQUERIDO: Tu project ID de GCP
region     = "us-central1"                     # Opcional: Región de GCP
zone       = "us-central1-a"                   # Opcional: Zona de GCP
environment = "dev"                            # Ambiente

# IMPORTANTE: Los nombres de buckets deben ser únicos globalmente
# Cambia "tu-proyecto" por algo único
input_bucket_name    = "tu-proyecto-input-files-dev"
output_bucket_name   = "tu-proyecto-processed-files-dev"
pubsub_topic_name    = "file-processing-topic"
function_name        = "file-processor-function"